{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "web-app/frontend/$1"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "vercel-python@3.10"
        }
    }
}